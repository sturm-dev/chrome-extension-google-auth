{
  "name": "plasmo-supabase-boilerplate",
  "displayName": "Plasmo supabase boilerplate",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "sturmenta@gmail.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.33.1",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@plasmohq/storage": "1.8.0",
    "@plasmohq/messaging": "0.5.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
